{
  "name": "ecfr-analytics-backend",
  "version": "1.0.0",
  "description": "Backend API server for eCFR Analytics with real-time word counting",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-wordcounter": "node services/wordcounter.js",
    "test-fcc": "node -e \"const {testSpecificAgency} = require('./services/wordcounter.js'); testSpecificAgency('FCC');\"",
    "test-fda": "node -e \"const {testSpecificAgency} = require('./services/wordcounter.js'); testSpecificAgency('FDA');\""
  },
  "keywords": [
    "ecfr",
    "regulations",
    "api",
    "word-count",
    "federal-agencies"
  ],
  "author": "USDS Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
